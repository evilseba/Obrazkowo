<div class="panel panel-default">
<div class="panel-heading"><h2>Lista pinów</h2></div>
<!--<div class="panel-body">-->
<div id="pins" class="transitions-enabled ">
<!--  <thead>
    <tr>
      <th>Opis</th>
      <th colspan="3"></th>
    </tr>
  </thead>-->

    <% @pins.each do |pin| %>
    <div class="box">
      <div class="panel panel-default">
        <%= link_to image_tag(pin.image.url(:medium)), pin %>
          </p>
          <div class="panel-body">
        <%= pin.description %>
        </div>
        </p>
        <% if pin.user == current_user %>
        <div class="panel-footer">
        <%= link_to 'Edytuj ', edit_pin_path(pin) %>
        <%= link_to 'Usuń', pin, method: :delete, data: { confirm: 'Jesteś pewny/pewna?' } %> </p>
        </div>
        <% end %>
        </div>
       </div>
    <% end %>
</div>
<div class="center">
<%= will_paginate @posts, renderer: BootstrapPagination::Rails %>
</div>
<div class="panel-footer"><%= link_to 'Nowy Pin', new_pin_path %></div>
</div>
